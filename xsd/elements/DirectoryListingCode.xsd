<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.aapt.com.au/FrontierLink/xsd"
            xmlns="http://www.w3.org/2001/XMLSchema"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:simpleType name="DirectoryListingCode">
        <xsd:restriction base="xsd:string">
			<xsd:enumeration value="Listed directory entry"/> 
			<xsd:enumeration value="Unlisted Entry"/> 
			<xsd:enumeration value="Listed but address suppressed"/> 
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>